import net.minecraft.client.entity.EntityPlayerSP;

public class Regen extends Module{

	public Regen() {
		super("Regen",0,Category.COMBAT);
	}
	
	@Override
	public void onUpdate() {
		if(this.isToggled()){
			EntityPlayerSP p=player();
		}
	}

}
