package me.gamrboy4life.paradox.module.status;

import me.gamrboy4life.paradox.mods.impl.ModPerspective;
import me.gamrboy4life.paradox.module.Category;
import me.gamrboy4life.paradox.module.Module;

public class Perspective extends Module {

    public Perspective() {
        super("Perspective", 0, Category.STATUS);
    }

    @Override
    public void onUpdate() {
    	ModPerspective modperspective=new ModPerspective();
    	
    	
        modperspective.setEnabled(isToggled());
    }
}