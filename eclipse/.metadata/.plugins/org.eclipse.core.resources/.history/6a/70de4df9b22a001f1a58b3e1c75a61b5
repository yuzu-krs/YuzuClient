package me.gamrboy4life.paradox.module.misc;

import me.gamrboy4life.paradox.module.Category;
import me.gamrboy4life.paradox.module.Module;
import me.gamrboy4life.paradox.cosmetics.impl.CosmeticTopHat;

public class Hat extends Module {

    public Hat() {
        super("Hat", 0, Category.MISC);
    }
    
    @Override
    public void onRender() {
        CosmeticTopHat.setShouldRender(this.isToggled());
    }
}